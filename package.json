{
  "name": "expressapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./dist/app.js",
    "nodemon": "nodemon ./dist/app.js",
    "build": "tsc",
    "watch": "tsc -w",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:generate": "sequelize migration:generate --name ",
    "db:seed": "sequelize db:seed:all",
    "db:seed:generate": "sequelize seed:generate --name ",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:reset": "sequelize db:drop && sequelize-cli db:create && sequelize-cli db:migrate",
    "db:setup": "yarn db:reset && yarn db:seed"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.13",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.9",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "~2.6.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prettier": "^2.0.5",
    "sequelize": "^5.21.13",
    "typescript": "^3.9.5"
  },
  "description": "",
  "main": "index.js",
